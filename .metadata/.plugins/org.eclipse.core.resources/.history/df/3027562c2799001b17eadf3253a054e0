package Exe06;

import java.util.Scanner;

public class int_change {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		
		String name = scan.next();
		String address1 = scan.next();
		int address2 = scan.nextInt();
		String address3 = scan.next();
		int address4 = scan.nextInt();
		int age = scan.nextInt();
		double weight = scan.nextDouble();
		
		System.out.println("이름: " + name + "\n" + "주소" + address1 + " " + address2 + address3 + " " + address4 + "\n" + age + " " + weight);
		
		scan.close();
		
	}

}